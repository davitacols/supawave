{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "build": {
    "env": {
      "CI": "false",
      "REACT_APP_API_URL": "http://localhost:8000/api",
      "REACT_APP_PAYSTACK_PUBLIC_KEY": "pk_test_bd4b7f52afbcf7cf0fa68951c5b7c7dfafbf377d"
    }
  }
}